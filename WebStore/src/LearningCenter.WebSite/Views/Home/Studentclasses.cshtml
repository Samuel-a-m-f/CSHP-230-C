@model LearningCenter.WebSite.Models.IndexModel
@{
    ViewBag.Title = "Registered Classes";
}


<div>
    <h1>Student Classes</h1>
</div>

<div class="row">
    <Table class="table">
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Paid</th>

        </tr>
        @foreach (var item in Model.registeredClasses)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@string.Format("{0:C}", item.Price)</td>
            </tr>
        }


    </Table>
</div>